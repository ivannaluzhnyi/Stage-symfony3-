{# sym3/OC/AdminBundle/Resources/views/Advert/Default/base.html.twig #}

{% extends "OCAdminBundle:User:form.html.twig" %}
{% block body %}
<h3 class="text-center">Réponses</h3>
<table class="table table-striped table-bordered">
	<tr>
		<th>ID</th>
		<th>Enfants</th>
		<th>Amis</th>
		<th>Date naiss.</th>
		<th>Raison principale de ne pas donner</th>
		<th>Mots-clés incitant à donner</th>
	</tr>  
	   {% for quest in listQuest %}
	<tr>
		<td>
		
			{{ quest.id }}  
		</td>
		<td>
			{{ quest.enfants }}
		</td>
		<td>
			{{ quest.amis }}  
		</td>
		<td>
			{{ quest.annee }}  
		</td>
		<td>
			{{ quest.raison }} 
		</td>
		<td>
			 {% for quests in quest.inciter %}
          <ul><li> {{ quests}} </li></ul> 
      {% endfor %}

		</td>
	</tr>
	{% endfor %}
</table>
{% endblock %}
